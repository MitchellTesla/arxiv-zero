{
    "$schema": "http://json-schema.org/schema#",
    "title": "Baz",
    "type": "object",
    "required": [
        "id",
        "foo",
        "created"
    ],
    "properties": {
        "id": {
            "description": "The unique identifier for the baz.",
            "type": "integer"
        },
        "created": {
            "description": "The datetime when the baz was created.",
            "type": "string",
            "format": "date-time"
        },
        "foo": {
            "description": "Some additional foo.",
            "type": "string"
        }
    }
}
